#: src/interface.c:41
msgid "Canon BJF360 Status Monitor"
msgstr ""

#: src/interface.c:68
msgid "Collecting printer status. "
msgstr "Συλλογή στοιχείων για την κατάσταση του εκτυπωτή."

#: src/interface.c:77 src/smstat.c:149
msgid "Cartridge Type:"
msgstr "Τύπος φυσιγγίου:"

#: src/interface.c:131
msgid "Continue"
msgstr "Συνέχεια"

#: src/interface.c:138
msgid "Cancel Printing"
msgstr "Ακύρωση εκτύπωσης"

#: src/callbacks.c:371 src/support.c:130
